<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.oodcw.JoinClub">
   <children>
      <ChoiceBox fx:id="selectClub" layoutX="155.0" layoutY="163.0" prefHeight="45.0" prefWidth="289.0" />
      <Text layoutX="80.0" layoutY="134.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select a club you wish to join from below" textAlignment="CENTER">
         <font>
            <Font size="25.0" />
         </font>
      </Text>
      <Button fx:id="joinClub" layoutX="248.0" layoutY="246.0" mnemonicParsing="false" onAction="#onjoinClubButtonClick" prefHeight="46.0" prefWidth="102.0" text="Join" />
      <Text layoutX="240.0" layoutY="69.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Join A Club" textAlignment="CENTER">
         <font>
            <Font size="25.0" />
         </font>
      </Text>
      <Button fx:id="clearClub" layoutX="248.0" layoutY="303.0" mnemonicParsing="false" onAction="#onclearClubButtonClick" prefHeight="22.0" prefWidth="102.0" text="Clear" />
      <Text layoutX="159.0" layoutY="358.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please select clear to select a new club to join" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
