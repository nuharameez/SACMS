<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.oodcw.StudentMenu">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0">
         <image>
            <Image url="@SCAMS_Final_MAin.png" />
         </image>
      </ImageView>
      <Button fx:id="join" layoutX="216.0" layoutY="169.0" mnemonicParsing="false" onAction="#onJoinClick" prefHeight="42.0" prefWidth="142.0" style="-fx-background-color: #214469;" text="JOIN CLUBS" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Button layoutX="216.0" layoutY="240.0" mnemonicParsing="false" onAction="#signOutClick" prefHeight="42.0" prefWidth="142.0" style="-fx-background-color: #e76f51;" text="SIGN OUT" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
