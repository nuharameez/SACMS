<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.oodcw.ClubAdvisorMenu">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0">
         <image>
            <Image url="@SCAMS_Final_MAin.png" />
         </image>
      </ImageView>
      <Button layoutX="72.0" layoutY="166.0" mnemonicParsing="false" onAction="#handleCreateClubs" prefHeight="34.0" prefWidth="214.0" style="-fx-background-color: #214469;" text="CREATE CLUBS" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button layoutX="327.0" layoutY="166.0" mnemonicParsing="false" onAction="#handleManageClubs" prefHeight="34.0" prefWidth="214.0" style="-fx-background-color: #214469;" text="MANAGE CLUBS" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="Schedule" layoutX="203.0" layoutY="233.0" mnemonicParsing="false" onAction="#handleSchedule" prefHeight="34.0" prefWidth="214.0" style="-fx-background-color: #214469;" text="SCHEDULE EVENTS" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
   </children>
</AnchorPane>
